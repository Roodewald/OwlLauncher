<Window x:Class="OwlLauncher.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OwlLauncher"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="500">
    <Grid>
        <Button Content="Загрузить" HorizontalAlignment="Center" Margin="0,350,0,0" VerticalAlignment="Top" Click="DownloadButton_Click"/>
        <ProgressBar Name="downloadProgressBar" VerticalAlignment="Top" Margin="10,230,10,0" Height="49"/>
        <Label x:Name="downloadSpeedLabel" Content="Label" Margin="24,94,207,295"/>
        <Label x:Name="downloadedSizeLabel" Content="Label" Margin="24,173,127,209"/>
    </Grid>
</Window>
